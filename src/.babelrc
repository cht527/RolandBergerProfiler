{
    "presets": [
        [
            "latest",
            {
                "es2015": {
                    "modules": false
                }
            }
        ]
    ],
    "plugins": [
        // 解决多个地方使用相同代码导致打包重复的问题
        [
            "@babel/plugin-transform-runtime"
        ],
        "external-helpers"
    ],
    "ignore": [
        "node_modules/**"
    ]
}