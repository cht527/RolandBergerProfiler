{
  "name": "rolandbergergraph",
  "version": "1.0.0",
  "description": "罗兰贝格消费者价值观分布图",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "/lib",
    "/es"
  ],
  "scripts": {
    "commit": "git-cz",
    "build": "NODE_ENV=production rollup -c",
    "dev": "rollup -c -w",
    "lint-fix": "eslint --cache --ext .js,.jsx,.ts,.tsx --fix --format=pretty  && npm run lint:prettier",
    "lint:prettier": "npm run prettier &&  git diff && prettier --version && prettier --check \"src/**.{js,jsx,tsx,ts,less,json}\" --end-of-line auto",
    "prettier": "prettier --write \"**/**.{js,jsx,tsx,ts,less,json}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cht527/rolandBergerGraph.git"
  },
  "keywords": [
    "罗兰贝格",
    "rolandberger",
    "消费者价值观"
  ],
  "author": "Cao Haitao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cht527/rolandBergerGraph/issues"
  },
  "homepage": "https://github.com/cht527/rolandBergerGraph#readme",
  "dependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "d3": "^7.0.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "rollup-plugin-babel": "^4.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/d3": "^7.0.0",
    "commitlint-config-cz": "^0.13.2",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "prettier": "^2.3.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.3.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint-fix",
      "git add"
    ]
  }
}
